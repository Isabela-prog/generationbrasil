package colecoesDeDadosListas;

import java.util.ArrayList;

public class Listas {

	public static void main(String[] args) {
		ArrayList<String> nomes = new ArrayList<String>();
		
		nomes.add("Maite");
		nomes.add("Isabela");
		nomes.add("Luiza");
		
		System.out.println(nomes);
		System.out.println(nomes.get(0));
		
		
	}

}
