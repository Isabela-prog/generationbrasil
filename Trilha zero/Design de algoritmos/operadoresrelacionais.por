programa {
  funcao inicio() {
    inteiro a, b
    a = 5
    b = 10
    escreva("O valor de a é ", a, " e o valor de b é ", b)
    escreva("\na é maior que b? ", a > b)
    escreva("\na é menor que b? ", a < b)
    escreva("\na é igual a b? ", a == b)
    escreva("\na é maior ou igual que b? ", a >= b)
    escreva("\na é menor ou igual que b? ", a <= b)
    escreva("\na é diferente de b? ", a != b)
  }
}
